    owl:versionInfo "3.0.0" ;
    skos:definition "The Vegetation module supports representation of vegetation objects with vegetation-specific thematic classes. CityGMLs vegetation model distinguishes between solitary vegetation objects like trees, and vegetation areas which represent biotopes like forests or other plant communities."@en .

veg:PlantCover a owl:Class ;
    rdfs:label "PlantCover"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange sc:Measure ;
            owl:onProperty veg:averageHeight ],
        [ a owl:Restriction ;
            owl:allValuesFrom sc:Measure ;
            owl:onProperty veg:maxHeight ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange sc:Measure ;
            owl:onProperty veg:minHeight ],
        [ a owl:Restriction ;
            owl:allValuesFrom sc:Measure ;
            owl:onProperty veg:minHeight ],
        [ a owl:Restriction ;
            owl:allValuesFrom sc:Measure ;
            owl:onProperty veg:averageHeight ],
        [ a owl:Restriction ;
            owl:allValuesFrom veg:PlantCoverFunctionValue ;
            owl:onProperty <https://www.opengis.net/ont/citygml/common/function> ],
        [ a owl:Restriction ;
            owl:allValuesFrom veg:PlantCoverUsageValue ;
            owl:onProperty <https://www.opengis.net/ont/citygml/common/usage> ],
        [ a owl:Restriction ;
            owl:allValuesFrom veg:PlantCoverClassValue ;
            owl:onProperty <https://www.opengis.net/ont/citygml/common/class> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange sc:Measure ;
            owl:onProperty veg:maxHeight ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass veg:PlantCoverClassValue ;
            owl:onProperty <https://www.opengis.net/ont/citygml/common/class> ],
        veg:AbstractVegetationObject ;
    owl:disjointWith veg:SolitaryVegetationObject .

veg:SolitaryVegetationObjectClassValue a owl:Class ;
    rdfs:label "SolitaryVegetationObjectClassValue"@en ;
    rdfs:subClassOf skos:Concept ;
    skos:definition "SolitaryVegetationObjectClassValue is a code list used to further classify a SolitaryVegetationObject."@en .

veg:SolitaryVegetationObjectFunctionValue a owl:Class ;
    rdfs:label "SolitaryVegetationObjectFunctionValue"@en ;
    rdfs:subClassOf skos:Concept ;
    skos:definition "SolitaryVegetationObjectFunctionValue is a code list that enumerates the different purposes of a SolitaryVegetationObject."@en .

veg:SolitaryVegetationObjectUsageValue a owl:Class ;
    rdfs:label "SolitaryVegetationObjectUsageValue"@en ;
    rdfs:subClassOf skos:Concept ;
    skos:definition "SolitaryVegetationObjectUsageValue is a code list that enumerates the different uses of a SolitaryVegetationObject."@en .

veg:SolitaryVegetationObject a owl:Class ;
    rdfs:label "SolitaryVegetationObject"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange sc:Measure ;
            owl:onProperty veg:crownDiameter ],
        [ a owl:Restriction ;
            owl:allValuesFrom veg:PlantCoverClassValue ;
            owl:onProperty <https://www.opengis.net/ont/citygml/common/class> ],
        [ a owl:Restriction ;
            owl:allValuesFrom veg:SpeciesValue ;
            owl:onProperty veg:species ],
        [ a owl:Restriction ;
            owl:allValuesFrom sc:Measure ;
            owl:onProperty veg:crownDiameter ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange sc:Measure ;
            owl:onProperty veg:height ],
        [ a owl:Restriction ;
            owl:allValuesFrom veg:PlantCoverUsageValue ;
            owl:onProperty <https://www.opengis.net/ont/citygml/common/usage> ],
        [ a owl:Restriction ;
            owl:allValuesFrom sc:Measure ;
            owl:onProperty veg:maxRootBallDepth ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass veg:PlantCoverClassValue ;
            owl:onProperty <https://www.opengis.net/ont/citygml/common/class> ],
        [ a owl:Restriction ;
            owl:allValuesFrom sc:Measure ;
            owl:onProperty veg:trunkDiameter ],
        [ a owl:Restriction ;
            owl:allValuesFrom sc:Measure ;
            owl:onProperty veg:rootBallDiameter ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange sc:Measure ;
            owl:onProperty veg:maxRootBallDepth ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange sc:Measure ;
            owl:onProperty veg:rootBallDiameter ],
        [ a owl:Restriction ;
            owl:allValuesFrom sc:Measure ;
            owl:onProperty veg:height ],
        [ a owl:Restriction ;
            owl:allValuesFrom veg:PlantCoverFunctionValue ;
            owl:onProperty <https://www.opengis.net/ont/citygml/common/function> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass veg:SpeciesValue ;
            owl:onProperty veg:species ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange sc:Measure ;
            owl:onProperty veg:trunkDiameter ],
        veg:AbstractVegetationObject .

<https://www.opengis.net/ont/citygml/common/function> rdfs:label "function"@en ;
    skos:definition "Specifies the intended purposes of the PlantCover."@en ;
    schema:rangeIncludes veg:PlantCoverFunctionValue .

<https://www.opengis.net/ont/citygml/common/usage> rdfs:label "usage"@en ;
    skos:definition "Specifies the actual uses of the PlantCover."@en ;
    schema:rangeIncludes veg:PlantCoverUsageValue .

veg:AbstractVegetationObject a owl:Class ;
    rdfs:label "AbstractVegetationObject"@en ;
    iso19150-2:isAbstract true ;
    rdfs:subClassOf core:AbstractOccupiedSpace .

veg:averageHeight a owl:DatatypeProperty ;
    rdfs:label "averageHeight"@en ;
    rdfs:range sc:Measure ;
    skos:definition "Specifies the average height of the PlantCover."@en .

veg:crownDiameter a owl:DatatypeProperty ;
    rdfs:label "crownDiameter"@en ;
    rdfs:range sc:Measure ;
    skos:definition "Specifies the diameter of the SolitaryCityObject's crown."@en .

veg:height a owl:DatatypeProperty ;
    rdfs:label "height"@en ;
    rdfs:range sc:Measure ;
    skos:definition "Distance between the highest point of the vegetation object and the lowest point of the terrain at the bottom of the object."@en .

veg:maxHeight a owl:DatatypeProperty ;
    rdfs:label "maxHeight"@en ;
    rdfs:range sc:Measure ;
    skos:definition "Specifies the maximum height of the PlantCover."@en .

veg:maxRootBallDepth a owl:DatatypeProperty ;
    rdfs:label "maxRootBallDepth"@en ;
    rdfs:range sc:Measure ;
    skos:definition "Specifies the vertical distance between the lowest point of the SolitaryVegetationObject's root ball and the terrain surface."@en .

veg:minHeight a owl:DatatypeProperty ;
    rdfs:label "minHeight"@en ;
    rdfs:range sc:Measure ;
    skos:definition "Specifies the minimum height of the PlantCover."@en .

veg:rootBallDiameter a owl:DatatypeProperty ;
    rdfs:label "rootBallDiameter"@en ;
    rdfs:range sc:Measure ;
    skos:definition "Specifies the diameter of the SolitaryCityObject's root ball."@en .

veg:species a owl:ObjectProperty ;
    rdfs:label "species"@en ;
    rdfs:range veg:SpeciesValue ;
    skos:definition "Indicates the botanical name of the SolitaryVegetationObject."@en .

veg:trunkDiameter a owl:DatatypeProperty ;
    rdfs:label "trunkDiameter"@en ;
    rdfs:range sc:Measure ;
    skos:definition "Specifies the diameter of the SolitaryCityObject's trunk."@en .

veg:PlantCoverFunctionValue a owl:Class ;
    rdfs:label "PlantCoverFunctionValue"@en ;
    rdfs:subClassOf skos:Concept ;
    skos:definition "PlantCoverFunctionValue is a code list that enumerates the different purposes of a PlantCover."@en .

veg:PlantCoverUsageValue a owl:Class ;
    rdfs:label "PlantCoverUsageValue"@en ;
    rdfs:subClassOf skos:Concept ;
    skos:definition "PlantCoverUsageValue is a code list that enumerates the different uses of a PlantCover."@en .

veg:SpeciesValue a owl:Class ;
    rdfs:label "SpeciesValue"@en ;
    rdfs:subClassOf skos:Concept ;
    skos:definition "A SpeciesValue is a code list that enumerates the species of a SolitaryVegetationObject."@en .

<https://www.opengis.net/ont/citygml/common/class> rdfs:label "class"@en ;
    skos:definition "Indicates the specific type of the PlantCover."@en ;
    schema:rangeIncludes veg:PlantCoverClassValue .

veg:PlantCoverClassValue a owl:Class ;
    rdfs:label "PlantCoverClassValue"@en ;
    rdfs:subClassOf skos:Concept ;
    skos:definition "PlantCoverClassValue is a code list used to further classify a PlantCover."@en .

@prefix core: <https://www.opengis.net/ont/citygml/core/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vers: <https://www.opengis.net/ont/citygml/versioning/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://www.opengis.net/ont/citygml/versioning> a owl:Ontology ;
    rdfs:label "Versioning"@en ;
    owl:imports <http://def.isotc211.org/iso19150/-2/2012/base>,
        <https://www.opengis.net/ont/citygml/common>,
        <https://www.opengis.net/ont/citygml/core> ;
    owl:versionIRI <https://www.opengis.net/ont/citygml/versioning/3.0.0/> ;
    owl:versionInfo "3.0.0" ;
    skos:definition "The Versioning module supports representation of multiple versions of CityGML features within a single CityGML model. In addition, also the version transitions and transactions that lead to the different versions can be represented."@en .

vers:TransactionTypeValue a owl:Class ;
    rdfs:label "TransactionTypeValue"@en ;
    rdfs:subClassOf skos:Concept ;
    owl:equivalentClass [ a rdfs:Datatype ;
            owl:oneOf ( "insert" "delete" "replace" ) ] ;
    skos:definition "TransactionTypeValue enumerates the three possible types of transactions: insert, delete, or replace."@en .

<https://www.opengis.net/ont/citygml/common/versionMember> skos:definition "Relates to all city objects that are part of the city model version."@en ;
    schema:domainIncludes vers:Version ;
    schema:rangeIncludes core:AbstractFeatureWithLifespan .

vers:tag a owl:DatatypeProperty ;
    rdfs:label "tag"@en ;
    rdfs:range xsd:string ;
    skos:definition "Allows for adding keywords to the city model version."@en .

vers:transaction a owl:ObjectProperty ;
    rdfs:label "transaction"@en ;
    rdfs:domain vers:VersionTransition ;
    rdfs:range vers:Transaction ;
    skos:definition "Relates to all transactions that have been applied as part of the VersionTransition."@en .

vers:clonePredecessor a owl:DatatypeProperty ;
    rdfs:label "clonePredecessor"@en ;
    rdfs:range xsd:boolean ;
    skos:definition "Indicates whether the set of city object instances belonging to the successor version of the city model is either explicitly enumerated within the successor version object (attribute clonePredecessor=false),  or has to be derived from the modifications of the city model provided as a list of transactions on the city object versions contained in the predecessor version (attribute clonePredecessor=true)."@en .

vers:from a owl:ObjectProperty ;
    rdfs:label "from"@en ;
    rdfs:domain vers:VersionTransition ;
    rdfs:range vers:Version ;
    skos:definition "Relates to the predecessor version of the VersionTransition."@en .

vers:newFeature a owl:ObjectProperty ;
    rdfs:label "newFeature"@en ;
    rdfs:domain vers:Transaction ;
    rdfs:range core:AbstractFeatureWithLifespan ;
    skos:definition "Relates to the version of the city object subsequent to the Transaction."@en .

vers:oldFeature a owl:ObjectProperty ;
    rdfs:label "oldFeature"@en ;
    rdfs:domain vers:Transaction ;
    rdfs:range core:AbstractFeatureWithLifespan ;
    skos:definition "Relates to the version of the city object prior to the Transaction."@en .

vers:reason a owl:DatatypeProperty ;
    rdfs:label "reason"@en ;
    rdfs:range xsd:string ;
    skos:definition "Specifies why the VersionTransition has been carried out."@en .

vers:to a owl:ObjectProperty ;
    rdfs:label "to"@en ;
    rdfs:domain vers:VersionTransition ;
    rdfs:range vers:Version ;
    skos:definition "Relates to the successor version of the VersionTransition."@en .

vers:VersionTransition a owl:Class ;
    rdfs:label "VersionTransition"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:boolean ;
            owl:onProperty vers:clonePredecessor ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty vers:reason ],
        [ a owl:Restriction ;
            owl:allValuesFrom vers:Transaction ;
            owl:onProperty vers:transaction ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty vers:reason ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:boolean ;
            owl:onProperty vers:clonePredecessor ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:allValuesFrom vers:TransitionTypeValue ;
            owl:onProperty vers:type ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass vers:Version ;
            owl:onProperty vers:from ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange vers:TransitionTypeValue ;
            owl:onProperty vers:type ],
        [ a owl:Restriction ;
            owl:allValuesFrom vers:Version ;
            owl:onProperty vers:to ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass vers:Version ;
            owl:onProperty vers:to ],
        [ a owl:Restriction ;
            owl:allValuesFrom vers:Version ;
            owl:onProperty vers:from ],
        core:AbstractVersionTransition .

vers:Transaction a owl:Class ;
    rdfs:label "Transaction"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom core:AbstractFeatureWithLifespan ;
            owl:onProperty vers:newFeature ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass core:AbstractFeatureWithLifespan ;
            owl:onProperty vers:newFeature ],
        [ a owl:Restriction ;
            owl:allValuesFrom core:AbstractFeatureWithLifespan ;
            owl:onProperty vers:oldFeature ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass core:AbstractFeatureWithLifespan ;
            owl:onProperty vers:oldFeature ],
        [ a owl:Restriction ;
            owl:allValuesFrom vers:TransitionTypeValue ;
            owl:onProperty vers:type ],
        [ a owl:Restriction ;
            owl:onDataRange vers:TransitionTypeValue ;
            owl:onProperty vers:type ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] ;
    skos:definition "Transaction represents a modification of the city model by the creation, termination, or replacement of a specific city object. While the creation of a city object also marks its first object version, the termination marks the end of existence of a real world object and, hence, also terminates the final version of a city object. The replacement of a city object means that a specific version of it is replaced by a new version."@en .

vers:type a owl:DatatypeProperty ;
    rdfs:label "type"@en ;
    rdfs:range vers:TransitionTypeValue ;
    skos:definition "Indicates the specific type of the VersionTransition."@en .

vers:TransitionTypeValue a rdfs:Datatype ;
    rdfs:label "TransitionTypeValue"@en ;
    owl:equivalentClass [ a rdfs:Datatype ;
            owl:oneOf ( "planned" "realized" "historicalSuccession" "fork" "merge" ) ] ;
    skos:definition "TransitionTypeValue enumerates the different kinds of version transitions. planned and fork should be used in cases when from one city model version multiple successor versions are being created. realized and merge should be used when different city model versions are converging into a common successor version."@en .

vers:Version a owl:Class ;
    rdfs:label "Version"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom core:AbstractFeatureWithLifespan ;
            owl:onProperty <https://www.opengis.net/ont/citygml/common/versionMember> ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty vers:tag ],
        core:AbstractVersion .

@prefix core: <https://www.opengis.net/ont/citygml/core/> .
@prefix iso19150-2: <http://def.isotc211.org/iso19150/-2/2012/base#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix wtr: <https://www.opengis.net/ont/citygml/waterbody/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://www.opengis.net/ont/citygml/waterbody> a owl:Ontology ;
    rdfs:label "WaterBody"@en ;
    owl:imports <http://def.isotc211.org/iso19150/-2/2012/base>,
        <https://www.opengis.net/ont/citygml/common>,
        <https://www.opengis.net/ont/citygml/core> ;
    owl:versionIRI <https://www.opengis.net/ont/citygml/waterbody/3.0.0/> ;
    owl:versionInfo "3.0.0" ;
    skos:definition "The WaterBody module supports representation of the thematic aspects and 3D geometry of rivers, canals, lakes, and basins. It does, however, not inherit any hydrological or other dynamic aspects of fluid flow."@en .

wtr:WaterGroundSurface a owl:Class ;
    rdfs:label "WaterGroundSurface"@en ;
    rdfs:subClassOf wtr:AbstractWaterBoundarySurface ;
    owl:disjointWith wtr:WaterSurface .

<https://www.opengis.net/ont/citygml/common/boundary> schema:domainIncludes wtr:WaterBody ;
    schema:rangeIncludes wtr:AbstractWaterBoundarySurface .

<https://www.opengis.net/ont/citygml/common/function> rdfs:label "function"@en ;
    skos:definition "Specifies the intended purposes of the WaterBody."@en ;
    schema:rangeIncludes wtr:WaterBodyFunctionValue .

<https://www.opengis.net/ont/citygml/common/usage> rdfs:label "usage"@en ;
    skos:definition "Specifies the actual uses of the WaterBody."@en ;
    schema:rangeIncludes wtr:WaterBodyUsageValue .

wtr:WaterBody a owl:Class ;
    rdfs:label "WaterBody"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom wtr:AbstractWaterBoundarySurface ;
            owl:onProperty <https://www.opengis.net/ont/citygml/common/boundary> ],
        [ a owl:Restriction ;
            owl:allValuesFrom wtr:WaterBodyUsageValue ;
            owl:onProperty <https://www.opengis.net/ont/citygml/common/usage> ],
        [ a owl:Restriction ;
            owl:allValuesFrom wtr:WaterBodyClassValue ;
            owl:onProperty <https://www.opengis.net/ont/citygml/common/class> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass wtr:WaterBodyClassValue ;
            owl:onProperty <https://www.opengis.net/ont/citygml/common/class> ],
        [ a owl:Restriction ;
            owl:allValuesFrom wtr:WaterBodyFunctionValue ;
            owl:onProperty <https://www.opengis.net/ont/citygml/common/function> ],
        core:AbstractOccupiedSpace .

wtr:WaterSurface a owl:Class ;
    rdfs:label "WaterSurface"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom wtr:WaterLevelValue ;
            owl:onProperty wtr:waterLevel ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass wtr:WaterLevelValue ;
            owl:onProperty wtr:waterLevel ],
        wtr:AbstractWaterBoundarySurface .

<https://www.opengis.net/ont/citygml/common/class> rdfs:label "class"@en ;
    skos:definition "Indicates the specific type of the WaterBody."@en ;
    schema:rangeIncludes wtr:WaterBodyClassValue .

wtr:WaterBodyFunctionValue a owl:Class ;
    rdfs:label "WaterBodyFunctionValue"@en ;
    rdfs:subClassOf skos:Concept ;
    skos:definition "WaterBodyFunctionValue is a code list that enumerates the different purposes of a WaterBody."@en .

wtr:WaterBodyUsageValue a owl:Class ;
    rdfs:label "WaterBodyUsageValue"@en ;
    rdfs:subClassOf skos:Concept ;
    skos:definition "WaterBodyUsageValue is a code list that enumerates the different uses of a WaterBody."@en .

wtr:waterLevel a owl:ObjectProperty ;
    rdfs:label "waterLevel"@en ;
    rdfs:range wtr:WaterLevelValue ;
    skos:definition "Specifies the level of the WaterSurface."@en .

wtr:WaterBodyClassValue a owl:Class ;
    rdfs:label "WaterBodyClassValue"@en ;
    rdfs:subClassOf skos:Concept ;
    skos:definition "WaterBodyClassValue is a code list used to further classify a WaterBody."@en .

wtr:WaterLevelValue a owl:Class ;
    rdfs:label "WaterLevelValue"@en ;
    rdfs:subClassOf skos:Concept ;
    skos:definition "WaterLevelValue is a code list that enumerates the different levels of a water surface."@en .

wtr:AbstractWaterBoundarySurface a owl:Class ;
    rdfs:label "AbstractWaterBoundarySurface"@en ;
    iso19150-2:isAbstract true ;
    rdfs:subClassOf core:AbstractThematicSurface .

@prefix core: <https://www.opengis.net/ont/citygml/core/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vers: <https://www.opengis.net/ont/citygml/versioning/> .
@prefix wksp: <https://www.opengis.net/ont/citygml/workspace/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://www.opengis.net/ont/citygml/workspace> a owl:Ontology ;
    rdfs:label "Workspace"@en ;
    owl:imports <http://def.isotc211.org/iso19150/-2/2012/base>,
        <https://www.opengis.net/ont/citygml/common>,
        <https://www.opengis.net/ont/citygml/core>,
        <https://www.opengis.net/ont/citygml/versioning> ;
    owl:versionIRI <https://www.opengis.net/ont/citygml/workspace/3.0.0/> ;
    owl:versionInfo "3.0.0" ;
    skos:definition "This pattern provides a starting point for modeling a GML Application Schema which is a specialized xml schema that uses the Geographic Markup Language to model Features to provide the basis for interoperability within a community of interest."@en .

wksp:ConcensusSpace a owl:Class ;
    rdfs:label "ConcensusSpace"@en ;
    rdfs:subClassOf wksp:Space ;
    owl:disjointWith wksp:PropositionSpace .

<https://www.opengis.net/ont/citygml/common/versionMember> schema:domainIncludes wksp:Scenario ;
    schema:rangeIncludes vers:Version .

wksp:PropositionSpace a owl:Class ;
    rdfs:label "PropositionSpace"@en ;
    rdfs:subClassOf wksp:Space .

wksp:Workspace a owl:Class ;
    rdfs:label "Workspace"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom wksp:Space ;
            owl:onProperty wksp:spaceMember ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
            owl:onClass wksp:Space ;
            owl:onProperty wksp:spaceMember ],
        [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass wksp:Space ;
            owl:onProperty wksp:spaceMember ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty wksp:creator ],
        core:AbstractFeatureWithLifespan .

wksp:versionTransitionMember a owl:ObjectProperty ;
    rdfs:label "versionTransitionMember"@en ;
    rdfs:domain wksp:Scenario ;
    rdfs:range vers:VersionTransition .

wksp:scenarioMember a owl:ObjectProperty ;
    rdfs:label "scenarioMember"@en ;
    rdfs:domain wksp:Space ;
    rdfs:range wksp:Scenario .

wksp:creator a owl:DatatypeProperty ;
    rdfs:label "creator"@en ;
    rdfs:range xsd:string .

wksp:spaceMember a owl:ObjectProperty ;
    rdfs:label "spaceMember"@en ;
    rdfs:domain wksp:Workspace ;
    rdfs:range wksp:Space .

wksp:Scenario a owl:Class ;
    rdfs:label "Scenario"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom vers:VersionTransition ;
            owl:onProperty wksp:versionTransitionMember ],
        [ a owl:Restriction ;
            owl:allValuesFrom vers:Version ;
            owl:onProperty <https://www.opengis.net/ont/citygml/common/versionMember> ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty wksp:creator ],
        core:AbstractFeatureWithLifespan .

wksp:Space a owl:Class ;
    rdfs:label "Space"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass wksp:Scenario ;
            owl:onProperty wksp:scenarioMember ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty wksp:creator ],
        [ a owl:Restriction ;
            owl:allValuesFrom wksp:Scenario ;
            owl:onProperty wksp:scenarioMember ],
        core:AbstractFeatureWithLifespan .

