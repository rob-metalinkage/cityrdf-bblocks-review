        [ a owl:Restriction ;
            owl:allValuesFrom tun:TunnelConstructiveElement ;
            owl:onProperty tun:tunnelConstructiveElement ],
        [ a owl:Restriction ;
            owl:allValuesFrom tun:HollowSpaceUsageValue ;
            owl:onProperty <https://www.opengis.net/ont/citygml/common/usage> ],
